<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.usbxyz.usb2xxxdemo">

    <uses-feature
        android:name="android.hardware.usb.host"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".USB2GPIO.GPIOTestActivity" />
        <activity android:name=".USB2GPIO.USB2GPIOActivity" />
        <activity android:name=".USB2CAN.USB2CANActivity" />
        <activity android:name=".USB2CAN.CANTestActivity" />
        <activity android:name=".USB2ADC.USB2ADCActivity" />
        <activity android:name=".USB2IIC.USB2IICActivity" />
        <activity android:name=".USB2PWM.USB2PWMActivity" />
        <activity android:name=".USB2SPI.USB2SPIActivity" />
        <activity android:name=".USB2UART.USB2UARTActivity" />
        <activity android:name=".USB2LIN.USB2LINActivity" />
        <activity android:name=".USB2ADC.ADCTestActivity" />
        <activity android:name=".USB2IIC.AT24C02Activity" />
        <activity android:name=".USB2LIN.LINMasterActivity" />
        <activity android:name=".USB2PWM.PWMTestActivity" />
        <activity android:name=".USB2SPI.SPIMasterTestActivity" />
        <activity android:name=".USB2UART.UARTTestActivity" />
        <activity android:name=".USB2SPI.W25Q32Activity"></activity>
    </application>

</manifest>